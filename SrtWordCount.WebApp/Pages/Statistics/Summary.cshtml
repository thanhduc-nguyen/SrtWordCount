@page
@model SrtWordCount.WebApp.Pages.SummaryModel
@{
    ViewData["Title"] = "Summary";
}

<h2>Summary</h2>

<h5 class="mt-3">Total distinct words</h5>
@foreach (var item in Model.SrtStatisticsModels)
{
    <div>@item.MovieTitle which is a @item.Genre movie has @item.TotalDistictWordCounts words.</div>
}